        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.BoletimNewActivity"
            android:label="@string/title_activity_boletim_new"
            android:theme="@style/AppTheme.NoActionBar"></activity>
        <activity
            android:name=".Activity.UsuarioNewActivity"
            android:label="@string/title_activity_usuario_new"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.UsuarioActivity"
            android:label="@string/title_activity_usuario"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.TipoColetaNewActivity"
            android:label="@string/title_activity_tipo_coleta_new"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.TipoColetaActivity"
            android:label="@string/title_activity_tipo_coleta"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.TurmaNewActivity"
            android:label="@string/title_activity_turma_new"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.TurmaActivity"
            android:label="@string/title_activity_turma"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.EstagioNewActivity"
            android:label="@string/title_activity_estagio_new"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.EstagioActivity"
            android:label="@string/title_activity_estagio"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.PragaNewActivity"
            android:label="@string/title_activity_praga_new"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Activity.PragaActivity"
            android:label="@string/title_activity_praga"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".HomeActivity"
            android:label="@string/title_activity_home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>